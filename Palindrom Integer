class Solution {
    public boolean isPalindrome(int x) {
        if(x<0){
            return false;
        }
        
        String a=""; 
            a= ((Integer)x).toString();
        
        for(int i=0;i<a.length()/2;i++){
            if(a.charAt(i)!=a.charAt(a.length()-i-1)){
                return false;
            }
            
            
        }
        
        return true;
    }
}
